{
    "env": {
        "browser": true,
        "es2023": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": [
            "error",
            "always"
        ],
        "no-unreachable": "warn",
        "no-unused-vars": [
            1,
            {
                "vars": "all",
                "args": "none"
            }
        ]
    }
}